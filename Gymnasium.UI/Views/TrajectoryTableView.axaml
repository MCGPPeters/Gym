<UserControl xmlns="https://github.com/avaloniaui"
             x:Class="Gymnasium.UI.Views.TrajectoryTableView">
    <DataGrid Name="TrajectoryTable" AutoGenerateColumns="False" CanUserSortColumns="True" Height="200">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Step" Binding="Step" Width="*"/>
            <DataGridTextColumn Header="State" Binding="State" Width="*"/>
            <DataGridTextColumn Header="Action" Binding="Action" Width="*"/>
            <DataGridTextColumn Header="Reward" Binding="Reward" Width="*"/>
        </DataGrid.Columns>
    </DataGrid>
</UserControl>
